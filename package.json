{
  "name": "react-redux-universal-boilerplate",
  "version": "1.0.0",
  "description": "React Universal Boilerplate",
  "engines": {
    "node": ">=5.1.0"
  },
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node ./dist/server/index.js",
    "dev": "NODE_ENV=development babel-node ./src/server",
    "build": "npm run build:server && npm run build:client",
    "build:client": "webpack -p --config ./build/webpack/webpack.prod.config.js",
    "build:server": "babel src/server --out-dir dist/server && babel src/common --out-dir dist/common && webpack -p --config ./build/webpack/webpack.server.config.js"
  },
  "author": "Will McMahan",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-transform-runtime": "6.1.18",
    "babel-polyfill": "^6.1.19",
    "body-parser": "1.14.1",
    "cookie-parser": "1.4.0",
    "express": "4.13.3",
    "history": "1.13.1",
    "isomorphic-fetch": "^2.2.0",
    "jade": "1.11.0",
    "match-media-mock": "^0.1.0",
    "morgan": "1.6.1",
    "radium": "0.15.3",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-redux": "^4.0.0",
    "react-router": "1.0.0",
    "redux": "^3.0.4",
    "redux-devtools": "^2.1.5",
    "redux-router": "^1.0.0-beta4",
    "redux-thunk": "^1.0.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "devDependencies": {
    "babel-cli": "6.1.18",
    "babel-core": "6.1.21",
    "babel-loader": "6.1.0",
    "babel-preset-es2015": "6.1.18",
    "babel-preset-react": "6.1.18",
    "babel-preset-stage-1": "6.1.18"
  }
}
